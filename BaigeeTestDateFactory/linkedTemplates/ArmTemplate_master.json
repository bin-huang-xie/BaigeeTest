{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "数据工厂名称",
			"defaultValue": "BaigeeTestDateFactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "“AzureBlobStorage1”的“connectionString”的安全字符串"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "“AzureSynapseAnalytics1”的“connectionString”的安全字符串"
		},
		"Trigger_63j_properties_CopyPipeline_63j_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@{addhours(trigger().scheduledTime, -24)}"
		},
		"Trigger_63j_properties_CopyPipeline_63j_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "包含链接 ARM 模板的存储帐户的 URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "包含链接 ARM 模板的存储帐户的 SAS 令牌"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "BaigeeTestDateFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"Trigger_63j_properties_CopyPipeline_63j_parameters_windowStart": {
						"value": "[parameters('Trigger_63j_properties_CopyPipeline_63j_parameters_windowStart')]"
					},
					"Trigger_63j_properties_CopyPipeline_63j_parameters_windowEnd": {
						"value": "[parameters('Trigger_63j_properties_CopyPipeline_63j_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}